    "fastify": "^3.19.0",
    "form-data": "^3.0.1"
  },
  "devDependencies": {
    "@swc/core": "^1.2.135",
    "@swc/jest": "^0.2.17",
    "@types/node": "^16.3.1",
    "ava": "^4.3.1",
    "ts-node": "^10.9.1",
    "typescript": "^4.5.5"
  },
  "ava": {
    "extensions": {
      "ts": "module"
    },
    "nodeArguments": [
      "--loader=ts-node/esm"
