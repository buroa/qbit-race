{
  "name": "qbit-race",
  "version": "1.0.0",
  "description": "Qbit utilities for racing",
  "main": "./bin/index.js",
  "type": "module",
  "scripts": {
    "build": "tsc -p .",
    "backup": "node bin/backup.js",
    "dev": "nodemon src/index.ts",
    "start": "node build/index.js",
    "validate": "node tests/validate.js",
    "tag_unreg": "node bin/tag_errored.js",
    "test-dev": "ts-node src/index.ts",
    "test": "ava"
  },
  "author": "Raghu Saxena",
  "license": "ISC",
  "dependencies": {
    "@ckcr4lyf/bencode-esm": "^0.0.2",
    "@ckcr4lyf/logger": "^0.0.3",
    "axios": "^0.21.1",
    "commander": "^9.4.0",
    "dotenv": "^8.2.0",
    "fastify": "^3.19.0",
    "form-data": "^3.0.1"
  },
  "devDependencies": {
    "@types/node": "^18.6.3",
    "@types/sinon": "^10.0.13",
    "ava": "^4.3.1",
    "nock": "^13.2.9",
    "sinon": "^14.0.0",
    "ts-node": "^10.9.1",
    "typescript": "^4.7.4"
  },
  "ava": {
    "extensions": {
      "ts": "module"
    },
    "nodeArguments": [
      "--loader=ts-node/esm"
    ]
  },
  "preferGlobal": true,
  "bin": {
    "qbit-race": "./bin/index.mjs"
  }
}
